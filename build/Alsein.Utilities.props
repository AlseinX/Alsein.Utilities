<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="../version.props"/>

  <PropertyGroup>
    <Title>Alsein's .NET Programming Utilities</Title>
    <Description>Some useful utilities for .net programming.</Description>
    <LangVersion>8.0</LangVersion>
    <AssemblyName>$(MSBuildProjectName)</AssemblyName>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <RootNamespace>Alsein.Utilities</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Alsein</Authors>
    <Copyright>Copyright &#x00A9; Alsein.  All rights reserved.</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GitRepository>AlseinX/Alsein.Utilities</GitRepository>
    <PackageProjectUrl>https://github.com/$(GitRepository)</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/$(GitRepository)/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/$(GitRepository)/master/Alsein.png</PackageIconUrl>
    <RepositoryUrl>$(PackageProjectUrl).git</RepositoryUrl>
    <MinClientVersion>4.3</MinClientVersion>
    <PackageOutputPath>$(MSBuildProjectDirectory)/../../dist</PackageOutputPath>
  </PropertyGroup>

</Project>
